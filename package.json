{
  "name": "quiz",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.64",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-filename-rules": "^1.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-storybook": "^0.5.12",
    "prettier": "^2.6.2",
    "react-scripts": "5.0.1",
    "typescript": "^4.8.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "build": "react-scripts build",
    "start": "react-scripts start",
    "lint:es": "eslint \"src/**/*.{js,ts,tsx}\" --format=html -o lint_output/es_lint_output.html",
    "lint:ts": "tsc --noEmit --project tsconfig.json",
    "lint": "run-s lint:es lint:ts",
    "prettier:fix": "prettier --write \"src/**/*.{js,ts,tsx,json}\"",
    "format": "run-s prettier:fix",
    "test": "TZ=America/Los_Angeles react-scripts test --watchAll=false --coverage --forceExit --detectOpenHandles --env=jsdom --colors",
    "test:watch": "TZ=America/Los_Angeles react-scripts test --watchAll=true --coverage --detectOpenHandles --env=jsdom --colors",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "lts/fermium"
  }
}
